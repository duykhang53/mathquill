{
    "compilerOptions": {
      "noImplicitThis": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "strictNullChecks": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "strictPropertyInitialization": false,
      "strict": true,
      "target": "es2015",
      "lib": ["ES2015", "ScriptHost", "DOM", "ES2015.Collection"],
      "rootDir": "src",
      "baseUrl": ".",
      "paths": {
        "tslib": ["node_modules/tslib/tslib"],
      },
      "importHelpers": true,
      "noEmit": true,
      "pretty": true
    }
  }